<a href="/bandeja/{{_id}}">
    <div>
        <div class="swiper-container">
            <div class="swiper-wrapper">
                {{#each fotos}}
                    <img src="https://res.cloudinary.com/dm4dzbfxs/image/upload/{{img_url}}" alt="" class="img-fluid">           
                {{/each}}
            </div>
        </div>
    </div>

    <div class="card-footer">
        <h1>{{band_name}}</h1>
        <p>{{band_descr}}</p>
        <span class="price">{{format band_price}}</span>
        <small>{{medidas band_medidas}}</small>
    </div>
</a>

<script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.4.6/js/swiper.min.js"></script>
<script>
    var swiper = new Swiper('.swiper-container', {
        cubeEffect: {
            slideShadows: false,
        },
    })
</script>